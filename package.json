{
    "name": "project-champions-league",
    "version": "1.0.0",
    "description": "API Champions League with Framework Express",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "dist": "tsup src",
        "fix": "prettier . --write && eslint --fix .",
        "prepare": "husky install",
        "start:dev": "tsx --env-file=.env.dev src/server.ts",
        "start:watch": "tsx watch --env-file=.env.dev src/server.ts",
        "start:dist": "npm run dist && node dist/src/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samuelmarco-dev/project-champions-league.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/samuelmarco-dev/project-champions-league/issues"
    },
    "homepage": "https://github.com/samuelmarco-dev/project-champions-league#readme",
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.14.12",
        "@typescript-eslint/eslint-plugin": "^7.17.0",
        "@typescript-eslint/parser": "^7.17.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^9.1.1",
        "prettier": "^3.3.3",
        "tsup": "^8.2.2",
        "tsx": "^4.16.2",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "express": "^4.19.2"
    }
}
